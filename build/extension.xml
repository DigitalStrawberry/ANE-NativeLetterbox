<extension xmlns="http://ns.adobe.com/air/extension/19.0">
  <id>com.digitalstrawberry.ane.letterbox</id>
  <versionNumber>1.0.0</versionNumber>
  <platforms>
     <platform name="iPhone-ARM">
        <applicationDeployment>
            <nativeLibrary>libNativeLetterbox.a</nativeLibrary>
            <initializer>NativeLetterboxInitializer</initializer>
            <finalizer>NativeLetterboxFinalizer</finalizer>
        </applicationDeployment>
    </platform> 

    <platform name="iPhone-x86">
        <applicationDeployment>
            <nativeLibrary>libNativeLetterbox.a</nativeLibrary>
            <initializer>NativeLetterboxInitializer</initializer>
            <finalizer>NativeLetterboxFinalizer</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="default">
        <applicationDeployment/>
    </platform>
  </platforms>
</extension>
